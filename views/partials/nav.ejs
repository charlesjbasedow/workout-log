  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <% if (user) { %>
      <a href="/auth/logout">
        <button type="submit">Logout</button>
      </a>
    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
    <a 
      href="/workouts" <%- title === 'All Workouts' ? 'class="active"' : '' %>
    >All Workouts</a>
    <a 
      href="/workouts/new" <%- title === 'Add Workout' ? 'class="active"' : '' %>
    >Add Workout</a>
    <a 
			href="/goals/new"
			<%- title === 'Goals' ? 'class="active"' : '' %>
		>
			Edit Goals
		</a>
  </nav>
